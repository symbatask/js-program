s }\r\n    case GET_TASKS:\r\n      return { ...state, tasks: action.tasks }\r\n    default:\r\n      return state\r\n  }\r\n}\r\n\r\nexport function getWeeks() {\r\n  return (dispatch) => {\r\n    axios.get('/api/v1/weeks').then(({ data: weeks }) => dispatch({ type: GET_WEEKS, weeks }))\r\n  }\r\n}\r\n\r\nexport function getTasks(weekId) {\r\n  return (dispatch) => {\r\n    axios\r\n      .get(`/api/v1/weeks/${weekId}`)\r\n      .then(({ data: tasks }) => dispatch({ type: GET_TASKS, tasks }))\r\n  }\r\n}\r\n","export const GET_HOMEWORKS = 'GET_HOMEWORKS'\r\nexport const GET_COMMENTS = 'GET_COMMENTS'\r\nexport const CREATE_NEW_COMMENT = 'CREATE_NEW_COMMENT'\r\nexport const DELETE_COMMENT = 'DELETE_COMMENT'\r\n\r\nexport const GET_WEEKS = 'GET_WEEKS'\r\nexport const GET_TASKS = 'GET_TASKS'\r\nexport const GET_TASK = 'GET_TASK'\r\n\r\nexport const SIGN_IN = 'SIGN_IN'\r\nexport const TRY_SIGN_IN = 'TRY_SIGN_IN'\r\nexport const LOG_OUT = 'LOG_OUT'\r\n\r\nexport const GET_USERS = 'GET_USERS'\r\nexport const COMPLETE_HOMEWORK = 'COMPLETE_HOMEWORK'","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","import { NavLink } from \"react-router-dom\";\r\n\r\nconst HeaderLink = ({link, children}) => {\r\n  return <NavLink to={link}\r\n                  className=\"block mt-4 lg:inline-block lg:mt-0 text-white hover:text-blue-300 mr-4\"\r\n  >{children}</NavLink>\r\n}\r\n\r\nexport default HeaderLink","import {useSelector} from \"react-redux\";\r\nimport cx from \"classnames\";\r\nimport styles from './leaderboard.module.scss'\r\n\r\n\r\nconst Leaderboard = ({setIsOpenBoard, isOpenBoard, userName}) => {\r\n  const users = useSelector((s) => s.users.users)\r\n\r\n  return (\r\n    <>\r\n      <div onClick={() => setIsOpenBoard(false)}\r\n           className={cx(styles.leaderboard__backdrop, {[styles.open]: isOpenBoard})}/>\r\n      <div className={cx(styles.leaderboard, {[styles.open]: isOpenBoard})}>\r\n  